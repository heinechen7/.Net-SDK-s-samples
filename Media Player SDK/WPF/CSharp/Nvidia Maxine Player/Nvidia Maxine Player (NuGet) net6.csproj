<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>Nvidia_Maxine_Player</RootNamespace>
    <Nullable>disable</Nullable>
    <UseWPF>true</UseWPF>
    <RuntimeIdentifiers>win7-x64;win7-x86</RuntimeIdentifiers>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="VisioForge.DotNet.MediaPlayer" Version="15.5.25" />
    <PackageReference Include="VisioForge.DotNet.Core.NvidiaMaxine" Version="15.5.25" />
    <PackageReference Include="VisioForge.DotNet.Core.UI.WPF" Version="15.5.25" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.MediaPlayer.x64" Version="15.5.25" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.MediaPlayer.x86" Version="15.5.25" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.LAV.x64" Version="15.5.25" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.LAV.x86" Version="15.5.25" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="visioforge_main_icon.ico" />
  </ItemGroup>
</Project>
