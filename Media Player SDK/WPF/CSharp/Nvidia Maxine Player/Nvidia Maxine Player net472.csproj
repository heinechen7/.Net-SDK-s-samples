    <PackageReference Include="VisioForge.DotNet.Core.UI.WPF" Version="15.5.23" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="visioforge_main_icon.ico" />
  </ItemGroup>
</Project>
